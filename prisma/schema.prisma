// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Participant {
  ID   String @id @default(cuid())
  name String @unique
  type String
}

model nurseNote {
  ID       String   @id @default(cuid())
  time     DateTime @default(now())
  PID      String
  note     String
  timer    Int
  category String
}
